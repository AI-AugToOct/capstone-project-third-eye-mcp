# Third Eye MCP example environment file
# Copy to .env and populate secrets before running locally.

# API authentication
MCP_API_KEY=
THIRD_EYE_API_KEY=
THIRD_EYE_SECRET_KEY=
TENANCY_MODE=single
ADMIN_BOOTSTRAP_PASSWORD=

# Model providers
THIRD_EYE_PROVIDER=groq
GROQ_API_KEY=
OPENROUTER_API_KEY=

# Databases
DATABASE_URL=postgresql://user:password@hostname:5432/third_eye
REDIS_URL=redis://:password@hostname:6379/0

# Web + portals
PORTAL_BASE_URL=http://localhost:5173
CONTROL_PLANE_BASE_URL=http://localhost:5174
WS_BASE_URL=ws://localhost:8000
LAUNCH_PORTAL_ON_CONNECT=true
ALLOW_SHELL_OPEN=false

# Observability
LOG_LEVEL=INFO
METRICS_PORT=9090
HEALTH_PORT=8080
PROMETHEUS_SCRAPE_ENABLED=true

# Rate limiting defaults
RATE_LIMIT_USER_RPS=2
RATE_LIMIT_SESSION_RPS=2
BUDGET_TOKENS_MAX=120000
